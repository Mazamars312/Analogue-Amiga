CAPI=2:

name : ::picosoc:0

filesets:
  picosoc:
    files:
      - simpleuart.v
      - spimemio.v
      - picosoc.v
    file_type : verilogSource
    depend : [picorv32]

targets:
  default:
    filesets : [picosoc]
    parameters : [PICORV32_REGS, PICOSOC_MEM]

parameters:
  PICORV32_REGS:
    datatype : str
    default  : picosoc_regs
    paramtype : vlogdefine
  PICOSOC_MEM:
    datatype : str
    default : picosoc_mem
    paramtype : vlogdefine
